#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "用法：deadbeef [選項] [--] [檔案]\n"

#: ../main.c:85
#, c-format
msgid "Options:\n"
msgstr "選項:\n"

#: ../main.c:86
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  或  -h     顯示說明 (本訊息) 並結束\n"

#: ../main.c:87
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             結束播放器\n"

#: ../main.c:88
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          顯示版本資料並結束\n"

#: ../main.c:89
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             開始播放\n"

#: ../main.c:90
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             停止播放\n"

#: ../main.c:91
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            暫停播放\n"

#: ../main.c:92
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     切換暫停狀態\n"

#: ../main.c:93
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr "   --play-pause       如停止則開始播放，否則切換暫停狀態\n"

#: ../main.c:94
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             播放清單下一首歌\n"

#: ../main.c:95
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             播放清單上一首歌\n"

#: ../main.c:96
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           播放清單隨機歌曲\n"

#: ../main.c:97
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            將檔案附加至播放清單\n"

#: ../main.c:98
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr "   --gui PLUGIN       決定要用的圖像用戶介面，預設為 \"GTK2\"\n"

#: ../main.c:99
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT   將格式化的歌曲名輸出至 stdout\n"

#: ../main.c:100
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
